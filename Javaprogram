import java.util.*;

class TestClass {
    public static void main(String args[] ) {
      
     Scanner sc=new Scanner(System.in);
      int no=sc.nextInt();
      int temp;
      int flag=0;
      for(int i=2;i<=no/2;i++){
          temp=no%i;
          if(temp==0){
              flag=1;
              break;
          }
      }
      if(flag==0)
      System.out.println("Prime number");
      else
      System.out.println("Not a prime number");
    }
}
